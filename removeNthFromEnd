 public ListNode removeNthFromEnd(ListNode head, int n) {
        ListNode temp=head;
        int size=0;
        while(temp!=null)
        {
            temp=temp.next;
            size++;
        }
        int rem=size-n;
        if(rem==0)
        {
            head=head.next;
            return head;
        }
         ListNode temp1=head;
         for(int i=1;i<rem;i++)
         {
             temp1=temp1.next;
         }
         temp1.next=temp1.next.next;
         return head;
    }
